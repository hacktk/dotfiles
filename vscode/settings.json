{
    "editor.cursorBlinking": "blink",
    "editor.cursorStyle": "line-thin",
    "editor.folding": false,
    "editor.fontFamily": "Ricty",
    "editor.fontSize": 16,
    "editor.links": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [120],
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "php.suggest.basic": false,
    "search.useIgnoreFiles": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "Ricty",
    "terminal.integrated.fontSize": 16,
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "vscode-icons",
    "phpmd.validate.rulesets": "codesize,controversial,design,unusedcode",
    "phpmd.validate.executablePath": "./app/vendor",
    "phpmd.validate.rulesetsFile": "./app/phpmd.xml"
}
